{
  "name": "hanzisize-reboot-react",
  "version": "0.3.0",
  "private": true,
  "description": "Selectively enlarge text of a specific language",
  "main": "index.js",
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.11.2",
    "fontsource-roboto": "^4.0.0",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.2.0",
    "typescript": "^4.3.3"
  },
  "scripts": {
    "start": "set PORT=3000 && react-scripts start",
    "build": "react-scripts build && cd build && sed -i -e 's#https://storage[.]googleapis[.]com/workbox-cdn/releases/[0-9.]*/workbox-sw.js#/workbox-sw.js#g' service-worker.js && web-ext build && cd ..",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-version-patch": "mversion patch && cd public && mversion patch && cd ..",
    "update-version-minor": "mversion minor && cd public && mversion minor && cd ..",
    "update-version-major": "mversion major && cd public && mversion major && cd .."
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "clearMocks": true,
    "restoreMocks": true
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/chrome": "0.0.145",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "eslint-plugin-jquery": "^1.5.1",
    "react-devtools": "^4.8.2",
    "react-test-renderer": "^16.13.1",
    "web-ext": "^5.4.1"
  }
}
